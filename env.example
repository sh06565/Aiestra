# Environment Variables for Aiestra
# Copy this file to .env and fill in your actual values

# =============================================================================
# GOOGLE CALENDAR CONFIGURATION
# =============================================================================
# Your Google Calendar scheduling URL (get this from Google Calendar settings)
VITE_GOOGLE_CALENDAR_SCHEDULING_URL=https://calendar.google.com/calendar/appointments/schedules/YOUR_SCHEDULE_ID?gv=true

# Calendar button customization
VITE_CALENDAR_BUTTON_COLOR=#00FFC8
VITE_CALENDAR_BUTTON_LABEL=Book an appointment

# =============================================================================
# CONTACT INFORMATION
# =============================================================================
VITE_CONTACT_EMAIL=sales@aiestra.com
VITE_CONTACT_PHONE=+1 (555) 123-4567
VITE_CONTACT_ADDRESS=San Francisco, CA

# =============================================================================
# VAPI AI CONFIGURATION
# =============================================================================
# Your Vapi AI public key (get this from Vapi dashboard)
VITE_VAPI_PUBLIC_KEY=your_vapi_public_key_here

# Your Vapi AI assistant ID (get this from Vapi dashboard)
VITE_VAPI_ASSISTANT_ID=your_vapi_assistant_id_here

# Vapi Widget Appearance
VITE_VAPI_THEME=dark
VITE_VAPI_BASE_BG_COLOR=#000000
VITE_VAPI_ACCENT_COLOR=#14B8A6
VITE_VAPI_CTA_BUTTON_COLOR=#000000
VITE_VAPI_CTA_BUTTON_TEXT_COLOR=#ffffff
VITE_VAPI_BORDER_RADIUS=large
VITE_VAPI_SIZE=compact
VITE_VAPI_POSITION=bottom-right
VITE_VAPI_TITLE=AIESTRA
VITE_VAPI_START_BUTTON_TEXT=Start Call
VITE_VAPI_END_BUTTON_TEXT=End Call
VITE_VAPI_CTA_SUBTITLE=Aiestra is online for consultation
VITE_VAPI_CHAT_FIRST_MESSAGE=Hey, How can I help you today?
VITE_VAPI_CHAT_PLACEHOLDER=Type your message...
VITE_VAPI_VOICE_SHOW_TRANSCRIPT=true
VITE_VAPI_CONSENT_REQUIRED=true
VITE_VAPI_CONSENT_TITLE=Terms and conditions
VITE_VAPI_CONSENT_CONTENT=By clicking "Agree," and each time I interact with this AI agent, I consent to the recording, storage, and sharing of my communications with third-party service providers, and as otherwise described in our Terms of Service.
VITE_VAPI_CONSENT_STORAGE_KEY=vapi_widget_consent

# =============================================================================
# API KEYS (for future integrations)
# =============================================================================
# OpenAI API Key (for AI features)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for Claude integration)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI API Key (for Gemini integration)
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Stripe Keys (for payments)
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here
VITE_STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# SendGrid API Key (for email notifications)
VITE_SENDGRID_API_KEY=your_sendgrid_api_key_here

# =============================================================================
# DATABASE CONFIGURATION (for future backend)
# =============================================================================
VITE_DATABASE_URL=your_database_connection_string_here
VITE_DATABASE_USER=your_database_username_here
VITE_DATABASE_PASSWORD=your_database_password_here

# =============================================================================
# AUTHENTICATION (for future use)
# =============================================================================
VITE_AUTH_SECRET=your_auth_secret_here
VITE_JWT_SECRET=your_jwt_secret_here

# =============================================================================
# ANALYTICS AND TRACKING
# =============================================================================
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_MIXPANEL_TOKEN=your_mixpanel_token_here

# =============================================================================
# FEATURE FLAGS
# =============================================================================
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false
VITE_ENVIRONMENT=development

# =============================================================================
# SECURITY NOTES
# =============================================================================
# 1. Never commit your .env file to version control
# 2. Use different API keys for development and production
# 3. Rotate API keys regularly
# 4. Use environment-specific .env files (.env.development, .env.production)
# 5. Consider using a secrets management service for production 